<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="200"
             x:Class="APISandbox.Views.Orders.RJROrderView"
             xmlns:vm="using:APISandbox.ViewModels.Orders"
             x:DataType="vm:RJROrderViewModel">
  <StackPanel>
    <Grid RowDefinitions="20,20,20" ColumnDefinitions="100,100,200">
        <TextBlock Text="{Binding Symbol}" Grid.Row="0" Grid.Column="0"/>
        <TextBlock Text="{Binding Ordertype}" Grid.Row="0" Grid.Column="1"/>
        <TextBlock Text="{Binding Side}" Grid.Row="0" Grid.Column="2"/>
        <TextBlock Text="{Binding Qty}" Grid.Row="1" Grid.Column="0"/>
        <TextBlock Text="{Binding Price}" Grid.Row="1" Grid.Column="1"/>
        <TextBlock Text="{Binding Orderstatus}" Grid.Row="1" Grid.Column="2"/>
        <TextBlock Text="{Binding Cumexecqty}" Grid.Row="2" Grid.Column="0"/>
        <TextBlock Text="{Binding Id}" Grid.Row="2" Grid.Column="2"/>
      </Grid>
  </StackPanel>
</UserControl>
